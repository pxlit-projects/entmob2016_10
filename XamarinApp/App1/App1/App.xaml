    <ResourceDictionary>
      <converter:RoundConverter x:Key="localRoundConverter"></converter:RoundConverter>
      <converter:StringFormatConverter x:Key="localStringFormatConverter"></converter:StringFormatConverter>
    </ResourceDictionary>
  </Application.Resources>
</Application>